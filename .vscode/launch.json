{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": ["-e", "error", "1.txt"],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "${workspaceFolder}/src/grep",
      "program": "${workspaceFolder}/src/grep/s21_grep.exe",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb.exe",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}